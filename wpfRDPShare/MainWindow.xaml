<Window x:Class="wpfRDPShare.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfRDPShare"
        mc:Ignorable="d"
        Title="MainWindow" Height="180" Width="270">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="10,10,10,10">
            <Button x:Name="ShareButton" Width="100" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,10,10" Content="Share" Click="ShareButton_Click"></Button>
            <Button x:Name="DisShareButton" Width="100" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,10,10" Content="Close" Click="DisShareButton_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
